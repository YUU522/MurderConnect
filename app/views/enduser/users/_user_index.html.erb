<% users.each do |user| %>
   <% unless user.email == "guest@example.com" || user.is_deleted %>
    <div class="col-md-4 mb-4">
      <div class="card h-100">
        <div class="card-body">
          <h5 class="card-title">
            <%= link_to user.name, user_url(user) %>
          </h5>
          <p class="card-text"><%= user.bio %></p>
        </div>
      </div>
    </div>
  <% end %>
<% end %>