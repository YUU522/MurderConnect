<% unless current_enduser == enduser %>
  <% if current_enduser.following?(enduser) %>
    <%= link_to "フォロー外す", user_relationships_path(enduser.id), method: :delete, remote: true %>
  <% else %>
    <%= link_to "フォローする", user_relationships_path(enduser.id), method: :post, remote: true %>
  <% end %>
<% end %>