<%= form_with model: tag, url: admin_tags_path, local: false do |form| %>
  <div class="form-group">
    <%= form.label :tag_title %>
    <%= form.text_field :tag_title, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.submit "タグを新規作成", class: "btn btn-primary" %>
  </div>
<% end %>