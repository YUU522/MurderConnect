<% if @endusers.exists? %>
  <% @endusers.each do |enduser| %>
    <table>
      <thead>
        <tr>
          <th>name</th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= link_to enduser.name, user_path(enduser) %></td>
          <td>フォロー数: <%= enduser.followings.count %></td>
          <td>フォロワー数: <%= enduser.followers.count %></td>
        </tr>
      </tbody>
    </table>
  <% end %>
<% else %>
  <p>ユーザーはいません</p>
<% end %>