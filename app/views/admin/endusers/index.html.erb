
  <div class="container">
  <h1 class="mt-5 mb-4">会員一覧</h1>
  <div class="row">
    <% @users.each do |user| %>
      <% unless user.email == "guest@example.com" %>
        <div class="col-md-4 mb-4">
          <div class="card h-100">
            <div class="card-body">
              <h5 class="card-title">
                <%= link_to user.name, admin_enduser_path(user) %>
              </h5>
              <p class="card-text"><%= user.bio %></p>
            </div>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
